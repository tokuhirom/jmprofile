<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<GridPane alignment="center" hgap="10" prefHeight="640.0" prefWidth="760.0" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="me.geso.jmprofile.Controller">
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>
      <GridPane prefHeight="120.0" prefWidth="200.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="92.0" minWidth="10.0" prefWidth="78.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="140.0" minWidth="10.0" prefWidth="122.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints />
        </rowConstraints>
         <children>
            <TextField fx:id="hostField" onKeyTyped="#uriElementUpdated" text="localhost" GridPane.columnIndex="1" />
            <Label text="Host" />
            <Label text="User" GridPane.rowIndex="1" />
            <Label text="Password" GridPane.rowIndex="2" />
            <TextField fx:id="usernameField" text="root" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="passwordField" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <TextField fx:id="interval" text="0.5" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <Label text="Interval[sec]" GridPane.rowIndex="3" />
         </children>
         <GridPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </GridPane.margin>
      </GridPane>
      <GridPane GridPane.rowIndex="1" GridPane.vgrow="ALWAYS">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="startButton" mnemonicParsing="false" onAction="#doStart" text="Start" />
            <TextField fx:id="uriField" editable="false" GridPane.columnSpan="2" GridPane.rowIndex="2" />
         </children>
      </GridPane>
      <TableView fx:id="tableView" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowSpan="3" GridPane.vgrow="ALWAYS">
        <columns>
          <TableColumn fx:id="valueColumn" text="Count" />
          <TableColumn fx:id="queryColumn" minWidth="80.0" text="Query" />
        </columns>
      </TableView>
      <Label fx:id="stateLabel" text="Samples:" GridPane.rowIndex="2" />
   </children>
</GridPane>
